{
  "name": "Focus Blocker",
  "description": "Choose which sites to block to stay focused.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "https://developer.chrome.com/*"
  ]
}